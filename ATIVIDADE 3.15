#include <stdio.h>

int main() {
  int numero, potencia, total = 1, i = 0;;
  printf("Digite um número: ");
  scanf("%i", &numero);
  printf("Digite a potência deste número: ");
  scanf("%i", &potencia);

  while (i < potencia){
    total = total * numero;
    i++;
  }

  printf("%i elevado a %i é igual a: %i", numero, potencia, total);

  
  
    return 0;
}
